{
  "name": "Kickoff_workflow",
  "email_notifications": {
    "no_alert_for_skipped_runs": false
  },
  "webhook_notifications": {},
  "timeout_seconds": 0,
  "trigger": {
    "pause_status": "UNPAUSED",
    "periodic": {
      "interval": 1,
      "unit": "WEEKS"
    }
  },
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "bronze_matches",
      "run_if": "ALL_SUCCESS",
