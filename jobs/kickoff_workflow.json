    {
      "task_key": "kickoff_dashboard",
      "depends_on": [
        {
          "task_key": "gold_matches_teams"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "dashboard_task": {
        "subscription": {},
        "dashboard_id": "01f09d55ae8e1579bf846eaa264d6fcb"
      },
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    }
  ],
  "queue": {
    "enabled": true
  },
  "performance_target": "PERFORMANCE_OPTIMIZED",
  "run_as": {
    "user_name": "<EMAIL_PLACEHOLDER>"
  }
}
